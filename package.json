{
  "name": "@anchan828/nest-bull",
  "version": "0.3.0",
  "description": "The [Bull](https://github.com/OptimalBits/bull) module for [Nest](https://github.com/nestjs/nest).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc --watch",
    "test": "jest --runInBand --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --runInBand --coverage --watch --detectOpenHandles --forceExit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-bull.git"
  },
  "author": "Keigo Ando <anchan828@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anchan828/nest-bull/issues"
  },
  "homepage": "https://github.com/anchan828/nest-bull#readme",
  "dependencies": {
    "deepmerge": "^3.2.0",
    "fast-glob": "^2.2.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.2",
    "@nestjs/core": "^6.0.2",
    "bull": "^3.7.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/common": "^6.0.2",
    "@nestjs/core": "^6.0.2",
    "@nestjs/testing": "^6.0.3",
    "@types/bull": "^3.5.11",
    "@types/ioredis": "^4.0.10",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.7",
    "bull": "^3.7.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000",
    "ulid": "^2.3.0"
  }
}
