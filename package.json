    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/testing": "8.4.4",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.32",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "fast-glob": "3.2.11",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.0",
    "prettier": "2.6.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "turbo": "1.2.6",
    "typescript": "4.6.4",
    "ulid": "2.3.0"
  },
  "volta": {
    "node": "16.14.2"
  }
