    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "turbo": "1.11.3",
    "typescript": "5.3.3",
    "ulid": "2.3.0"
