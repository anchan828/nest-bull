{
  "name": "@anchan828/nest-bull-packages",
  "private": true,
  "version": "0.0.0",
  "description": "The [Bull](https://github.com/OptimalBits/bull) module for [Nest](https://github.com/nestjs/nest).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "lerna run --parallel lint",
    "lint:fix": "lerna run --parallel lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:minor": "lerna publish --yes minor",
    "publish:major": "lerna publish --yes major",
    "test": "lerna run test && npx @anchan828/merge-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-bull.git"
  },
  "author": "anchan828 <anchan828@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anchan828/nest-bull/issues"
  },
  "homepage": "https://github.com/anchan828/nest-bull#readme",
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nestjs/common": "8.0.1",
    "@nestjs/core": "8.0.1",
    "@nestjs/platform-express": "8.0.1",
    "@nestjs/testing": "8.0.1",
    "@types/ioredis": "4.26.5",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lerna": "4.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "ulid": "2.3.0",
    "rxjs": "7.2.0"
  }
}
