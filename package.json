    "@nestjs/core": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/testing": "10.3.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "fast-glob": "3.3.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
