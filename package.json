{
  "name": "@anchan828/nest-bull-packages",
  "private": true,
  "version": "0.0.0",
  "description": "The [Bull](https://github.com/OptimalBits/bull) module for [Nest](https://github.com/nestjs/nest).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "eslint --ignore-pattern packages '**/*.ts' && lerna run --parallel lint",
    "lint:fix": "eslint --ignore-pattern packages --fix '**/*.ts' && lerna run --parallel lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:minor": "lerna publish --yes minor",
    "publish:major": "lerna publish --yes major",
    "test": "lerna run test && ts-node scripts/merge-coverage.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-bull.git"
  },
  "author": "anchan828 <anchan828@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anchan828/nest-bull/issues"
  },
  "homepage": "https://github.com/anchan828/nest-bull#readme",
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@nestjs/common": "6.10.10",
    "@nestjs/core": "6.10.10",
    "@nestjs/platform-express": "6.10.10",
    "@nestjs/testing": "6.10.10",
    "@types/ioredis": "4.14.1",
    "@types/jest": "24.0.23",
    "@types/node": "11.15.3",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lerna": "3.19.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "reflect-metadata": "0.1.13",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.3",
    "ulid": "2.3.0"
  }
}
