{
  "name": "@anchan828/nest-bullmq-terminus",
  "version": "2.0.0",
  "description": "The terminus of [BullMQ](https://github.com/taskforcesh/bullmq) module for [Nest](https://github.com/nestjs/nest).",
  "homepage": "https://github.com/anchan828/nest-bull/tree/master/packages/bullmq-terminus#readme",
  "bugs": {
    "url": "https://github.com/anchan828/nest-bull/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-bull.git"
  },
  "license": "MIT",
  "author": "anchan828 <anchan828@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "copy:license": "cp ../../LICENSE ./",
    "lint": "TIMING=1 eslint --ignore-path ../../.eslintignore '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build && rm -f dist/*.tsbuildinfo && npm run copy:license",
    "test": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk ../../node_modules/jest/bin/jest --runInBand --logHeapUsage",
    "test:watch": "npm run test -- --watch",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@anchan828/nest-bullmq": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.3",
    "@nestjs/terminus": "9.0.0",
    "@types/supertest": "2.0.12",
    "bullmq": "1.86.5",
    "ioredis": "5.2.0",
    "rxjs": "7.5.6",
    "supertest": "6.2.4"
  },
  "peerDependencies": {
    "@nestjs/axios": "^0.0.8 || ^0.1.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0"
  }
}
