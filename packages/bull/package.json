  "dependencies": {
    "deepmerge": "3.2.0",
    "fast-glob": "2.2.6"
  },
  "devDependencies": {
    "@nestjs/common": "6.1.1",
    "@nestjs/core": "6.1.1",
    "@nestjs/testing": "6.1.1",
    "@types/bull": "3.5.13",
    "@types/ioredis": "4.0.10",
    "@types/jest": "24.0.12",
